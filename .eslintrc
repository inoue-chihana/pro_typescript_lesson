{
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "plugins": ["prettier", "import"],
    "rules": {
        "prettier/prettier": "error",
        "import/order": [
            "error",
            {
                "groups": [
                    "builtin",
                    "external",
                    "internal",
                    "parent",
                    "sibling",
                    "index",
                ],
                "newlines-between": "always",
            },
        ],
    },
}
